    "saikarthik parachi"
  ],
    "license": "ISC",
    "keywords": [
    "saikarthik",
    "parachi"
  ],
    "homepage": "https://github.com/sparachi/Learn-Node",
    "moduleType": [],
    "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
    "dependencies": {
        "bootstrap": "^3.3.6",
        "font-awesome": "^4.5.0"
    },
    "overrides": {
        "bootstrap": {
            "main": [
                "dist/js/bootstrap.js",
                "dist/css/bootstrap.min.css",
                "less/bootstrap.less"
